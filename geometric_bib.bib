	journal = {ACS Applied Energy Materials},
	author = {Wouk de Menezes, Luana Cristina and Jin, Yingzhi and Benatto, Leandro and Wang, Chuanfei and Koehler, Marlus and Zhang, Fengling and Roman, Lucimara Stolz},
