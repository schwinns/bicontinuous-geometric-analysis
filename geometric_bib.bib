	file = {Pindzola et al. - 2003 - Cross-Linked Normal Hexagonal and Bicontinuous Cub.pdf:/Users/nate_schwindt/Zotero/storage/FJ3CP8CH/Pindzola et al. - 2003 - Cross-Linked Normal Hexagonal and Bicontinuous Cub.pdf:application/pdf},
